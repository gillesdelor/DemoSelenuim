package dealership;

public class Cars {
	
	 //define variables
	protected  String color;
	protected int doors;
	protected String model;
	
	//default constructor
	public Cars (){
		
	}
		
    // parameterized constructor
	public Cars (String color, int doors, String model){
		this.color = color;
		this.doors = doors;
		this.model = model;
	}
	
	}
	
	
	
