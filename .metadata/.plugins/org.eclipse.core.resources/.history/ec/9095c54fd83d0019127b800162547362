package selectionstructure;

public class DualSelectionStructure {

	public static void main(String[] args) {
		// define variable
		int age = 10;
		//check if the user has the right to vote
		if (age >= 16){
			System.out.println("you can vote");
		}else{
			System.out.println("you are qulified to vote , you must be 16 years old to vote");
		}

	}

}
