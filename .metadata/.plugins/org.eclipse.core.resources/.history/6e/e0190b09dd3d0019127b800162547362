package selectionstructure;

public class MultipleSelectionStructure {

	public static void main(String[] args) {
		// define variables
		String city = "Dulles";
		if (city == "Dulles"){
		     System.out.println("This is the Washington Dulles International Airport");
		}else if (city == "Atlanta"){
			System.out.println("This is the harshField Jackson International Airport");
		}else if(city == " New Jersey"){
			System.out.println("This is the Newark International Airport");
		}else if (city == "Chicago"){
			System.out.println("This is the Chacago International Airport");
		}else
			System.out.println("This city "+city+" is incorrect");
		}

	}

